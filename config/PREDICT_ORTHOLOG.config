# ABOUT:  This is the configuration file for predicting ortholog and inparalog pairs and ortholog family clustering using DeNOGAP
# AUTHOR: Shalabh Thakur
# VERSION: 1.0
# Any thing that start with #  and between () will be ignored by the program.

[ACTIVATE_ANALYSIS]
-PREDICT_ORTHOLOG:YES
-CLUSTER_ORTHOLOG:YES

[GROUP]
-HMM_CLUSTER_FILE: ../output/TEST_RUN/HOMOLOG_SCAN/RESULT/model_cluster.txt
-HOMOLOG_CLUSTER_FILE: ../output/TEST_RUN/HOMOLOG_SCAN/RESULT/super_homolog_cluster.txt

[OUTPUT_DIRECTORY]
-PROJECT_DIR_NAME: HOMOLOG_SCAN

[PARAMETERS]
-ORTHOLOG_DIVERGENCE_THRESHOLD: 0.8
-INPARALOG_DIVERGENCE_THRESHOLD: 0.5
-TRANSITIVITY_CUTOFF: 100
-PARALLEL_CPU_CORE: 1











